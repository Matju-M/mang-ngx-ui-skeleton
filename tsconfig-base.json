{
    "compilerOptions": {
        "strict": true,
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "importHelpers": true,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "outDir": "./.artifact",
        "removeComments": true,
        "skipLibCheck": true,
        "lib": [
            "esnext",
            "dom"
        ]
    }
}