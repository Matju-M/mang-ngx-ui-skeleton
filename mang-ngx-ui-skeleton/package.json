{
  "name": "@mang/ngx-ui-skeleton",
  "version": "0.1.0",
  "description": "External NGx UI Library Skeleton",
  "keywords": [
    "skeleton",
    "mang"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && npm run lint && gulp copy:styles && rollup -c && rimraf dist/src",
    "aot": "gulp aot && npm run lint && npm run ngc && gulp clean:post",
    "ngc": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "lint": "tsfmt -r && tslint 'src/**/*.ts'",
    "test": "karma start"
  },
  "author": "Matthew Mangion",
  "license": "MIT",
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@angular/animations": "^5.0.0-rc",
    "@angular/common": "^5.0.0-rc",
    "@angular/compiler": "^5.0.0-rc",
    "@angular/compiler-cli": "^5.0.0-rc",
    "@angular/core": "^5.0.0-rc",
    "@angular/platform-browser": "^5.0.0-rc",
    "@angular/platform-browser-dynamic": "^5.0.0-rc",
    "@types/jasmine": "^2.6.0",
    "@types/karma": "^1.7.0",
    "@types/node": "^8.0.28",
    "codelyzer": "^3.2.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "gulp-sass": "^3.1.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.7",
    "karma-typescript-angular2-transform": "^1.0.2",
    "require-dir": "^0.3.2",
    "rollup": "^0.49.3",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-node-resolve-angular": "^2.0.3",
    "rollup-plugin-typescript2": "^0.5.2",
    "run-sequence": "^2.2.0",
    "rxjs": "^5.4.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "zone.js": "^0.8.14"
  },
  "files": [
    "dist"
  ]
}
